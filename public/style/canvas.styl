
purple = #7f00ff

#canvas
  position fixed
  left 0px
  top 0px
  z-index -1

path
  stroke #fff

pathfirst-child
  fill yellow !important

circle
  fill transparent
  pointer-events none

.q0-9
  fill lighten(purple, 20%)
.q1-9
  fill darken(purple, 20%)
.q2-9
  fill darken(purple, 10%)
.q3-9
  fill lighten(purple, 10%)
.q4-9
  fill darken(purple, 5%)
.q5-9
  fill lighten(purple, 5%)
.q6-9
  fill darken(purple, 15%)
.q7-9
  fill lighten(purple, 15%)
.q8-9
  fill purple
